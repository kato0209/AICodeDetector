{
    "name": "GPTSniffer-DEVC",
    "dockerFile": "../Dockerfile",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "GitHub.copilot"
            ]
        }
    },
    "workspaceFolder": "/home/kato/GPTSniffer",
    "remoteUser": "kato",
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/kato/GPTSniffer,type=bind,consistency=cached",
    "runArgs": ["--gpus", "all"]
}